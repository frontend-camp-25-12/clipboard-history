{
  "id": "com.example.clipboardhistory",
  "name": "剪贴板历史",
  "description": "记录剪贴板历史，支持文本、图片和文件的复制与粘贴",
  "version": "1.0.0",
  "logo": "./src/assets/icons/icon.png",
  "preload": "./dist/preload/preload.js",
  "content": "./dist/src/index.html",
  "background": true,
  "window": {
    "width": 800,
    "height": 600,
    "disableTransition": false,
    "frame": true,
    "transparent": false,
    "resizable": true
  },
  "features": [
    {
      "code": "openClipboardHistory",
      "label": "打开界面，查找想要复制的剪贴板历史",
      "hotKey": true,
      "searchable": true,
      "cmds": ["打开、搜索、复制剪贴板历史记录", "文本、图片、文件历史"]
    }
  ]
}
